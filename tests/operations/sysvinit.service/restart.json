{
    "args": ["nginx"],
    "kwargs": {
        "restarted": true
    },
    "facts": {
        "sysvinit.InitdStatus": {
            "nginx": true
        }
    },
    "commands": [
        "/etc/init.d/nginx restart"
    ],
    "idempotent": false,
    "disable_itempotent_warning_reason": "service restarts are always executed"
}
